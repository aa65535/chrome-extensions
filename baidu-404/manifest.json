{
	"name": "Baidu-404",
	"description": "解除百度部分产品对搜索引擎的屏蔽",
	"version": "1.0.1",
	"permissions": [ "<all_urls>", "tabs", "webRequestBlocking", "webRequest" ],
	"manifest_version": 2,
	"icons": {
		"16": "./icons/icon16.png",
		"32": "./icons/icon32.png",
		"48": "./icons/icon48.png"
	},
	"background": {
		"scripts": ["./script/background.js"],
		"persistent": true
	}
}
